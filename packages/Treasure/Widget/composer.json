{
    "name": "treasure/widget",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Onoja Treasure",
            "email": "onojatreasure@gmail.com",
            "homepage": "https://github.com/onojatreasure"
        }
    ],
    "homepage": "https://github.com/treasure/widget",
    "keywords": ["Laravel", "Widget"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Treasure\\Widget\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Treasure\\Widget\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Treasure\\Widget\\WidgetServiceProvider"
            ],
            "aliases": {
                "Widget": "Treasure\\Widget\\Facades\\Widget"
            }
        }
    }
}
